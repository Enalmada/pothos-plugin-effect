{
  "name": "pothos-plugin-effect",
  "description": "Seamless integration between the Pothos GraphQL and Effect.",
  "publishConfig": {
    "access": "public",
    "registiry": "https://registry.npmjs.org"
  },
  "version": "0.7.1",
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "build": "nanobundle build",
    "prepack": "pnpm build",
    "release": "release-it"
  },
  "keywords": [
    "effect",
    "pothos",
    "plugin"
  ],
  "author": {
    "name": "Chanhee Lee",
    "email": "iamchanii.0310@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@pothos/core": "^3.35.0",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-relay": "^3.43.1",
    "@swc/core": "^1.3.84",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "effect": "2.0.0-next.32",
    "eslint": "^8.49.0",
    "eslint-plugin-dprint-integration": "^0.3.0",
    "graphql": "^16.8.0",
    "graphql-yoga": "^4.0.4",
    "jest": "^29.7.0",
    "nanobundle": "^1.6.0",
    "release-it": "^16.1.5",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@pothos/core": "^3",
    "effect": ">=2.0.0-next.23 <3",
    "graphql": "^16 || ^17"
